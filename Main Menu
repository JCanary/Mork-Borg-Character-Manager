using RandomTables;

class MorkBorgManager
{
    public static void Main()
    {
        var tables = new Tables();

        Console.WriteLine($"Occult Treasure: {tables.OccultTreasures()}");
        Console.WriteLine($"Traps: {tables.TrapsAndDevilry()}");
        Console.WriteLine($"Weather: {tables.Weather()}");
        Console.WriteLine($"Plundering: {tables.CorpsePlundering()}");
        Console.WriteLine($"Armor: {tables.StartingArmor()}");
        Console.WriteLine($"Weapon: {tables.StartingWeapon()}");
        Console.WriteLine($"Starting item 1: {tables.StartingItem1()}");
        Console.WriteLine($"Starting item 2: {tables.StartingItem2()}");
        Console.WriteLine($"Starting item 3: {tables.StartingItem3()}");
        Console.WriteLine($"Basilisk's Demand: {tables.BasilisksDemand()}");
        Console.WriteLine($"Unclean Scroll: {tables.UncleanScrolls()}");
        Console.WriteLine($"Sacred Scroll: {tables.SacredScrolls()}");
        Console.WriteLine($"Terrible Trait: {tables.TerribleTraits()}");
        Console.WriteLine($"Broken Bodies: {tables.BrokenBodies()}");
        Console.WriteLine($"Bad Habit: {tables.BadHabits()}");
    }
}
